{
    "manifest_version": 3,
    "name": "PSMF helper",
    "version": "1.0",
    "description": "Export zápasů z PSMF do CSV",
    "icons": {
        "16": "images/icon16.png",
        "64": "images/icon64.png",
        "128": "images/icon128.png"
      },
    "content_scripts": [
        {
          "js": ["scripts/content.js"],
          "matches": [
            "https://www.psmf.cz/souteze/*"
          ]
        }
      ]
  }
  